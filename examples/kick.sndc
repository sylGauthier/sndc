lin: osc {
    function: "sin";
    freq: 1;
    duration: 0.25;
    sampling: 10;

    amplitude: 250;
    a_offset: 120;
    p_offset: 0.5;
}

pr: print {
    input: lin.out;
    file: "out";
}

wav: osc {
    function: "sin";
    freq: lin.out;
    duration: 1;
    amplitude: 1;
}

env: envelop {
    input: wav.out;
    attack: 0.02;
    sustain: 0;
    decay: 0.3;
}
