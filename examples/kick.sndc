freq: func {
    function: "exp";
    duration: 1;
    sampling: 100;

    param0: 180;
    param2: -3.;
}

wav: osc {
    function: "square";
    freq: freq.out;
    duration: 0.5;
    amplitude: 1;
}

fenv: func {
    function: "exp";
    duration: 1;
    sampling: 10;

    param0: 1000;
    param2: -2.;
}

f: filter {
    input: wav.out;
    lfcutoff: 0;
    hfcutoff: fenv.out;
}

env: envelop {
    input: f.out;
    attack: 0.01;
    sustain: 0;
    decay: 0.3;
}
